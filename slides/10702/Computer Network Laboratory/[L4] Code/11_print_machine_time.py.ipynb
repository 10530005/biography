    "import ntplib\n",
    "from time import ctime\n",
    "\n",
    "\n",
    "def print_time():\n",
    "    ntp_client = ntplib.NTPClient()\n",
    "    response = ntp_client.request('pool.ntp.org')\n",
    "    print (ctime(response.tx_time))\n",
    "\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    print_time()"
   ]
  }
